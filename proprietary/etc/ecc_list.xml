<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!-- 
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="112" Category="0" Condition="1" Mccmnc="cn"/>
    <EccEntry Ecc="911" Category="0" Condition="1" Mccmnc="cn"/>
    <EccEntry Ecc="000" Category="0" Condition="1" Mccmnc="cn"/>
    <EccEntry Ecc="08" Category="0" Condition="1" Mccmnc="cn"/>
    <!--modify by huangchunyan 150501-->
    <EccEntry Ecc="110" Category="0" Condition="2" Mccmnc="cn"/>
    <EccEntry Ecc="118" Category="0" Condition="1" Mccmnc="cn"/>
     <!--modify by huangchunyan 150501-->
    <!--modify by czw 20170821-->
    <EccEntry Ecc="120" Category="0" Condition="2" Mccmnc="cn"/>
        <!--add by lct huangchunyan-->
    <EccEntry Ecc="994" Category="0" Condition="2" Mccmnc="cn"/>
    <EccEntry Ecc="113" Category="0" Condition="2" Mccmnc="cn"/>
        <!--end-->
    <EccEntry Ecc="119" Category="0" Condition="2" Mccmnc="cn"/>
    <EccEntry Ecc="999" Category="0" Condition="2" Mccmnc="cn"/>
</EccTable>
